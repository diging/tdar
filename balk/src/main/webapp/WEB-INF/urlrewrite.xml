<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

	<rule>
		<from>/items/([^\?+])</from>
		<to>/items/?path=$1</to>
	</rule>
	<outbound-rule>
		<from>/items?path=(.+)</from>
		<to>/items/$1</to>
	
	</outbound-rule>
</urlrewrite>
