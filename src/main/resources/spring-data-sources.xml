<?xml version="1.0"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id='tdarGeoDatabase' class='org.tdar.search.geosearch.GeoSearchDao' lazy-init="true">
        <property name='dataSource' ref='tdarGeoDataSource' />
    </bean>
    <bean id='tdarDataImportDatabase' class='org.tdar.db.model.PostgresDatabase'>
        <property name='dataSource' ref='tdarDataImportDataSource' />
    </bean>
</beans>
