<?xml version="1.0"?>
<!--
vim:sts=2:sw=2:
-->
<decorators defaultdir="/">
    <excludes>
        <pattern>**/ajax/**</pattern>
        <pattern>/css/*</pattern>
        <pattern>/images/*</pattern>
        <pattern>/includes/*</pattern>
        <pattern>/js/*</pattern>
        <pattern>/*.html</pattern>
        <pattern>/map</pattern>
        <pattern>/featured</pattern>
        <pattern>/config-browser/*</pattern>
        <pattern>/services/*</pattern>
    </excludes>
    <!--  FIXME:// http://stackoverflow.com/questions/5329693/is-it-possible-to-decorate-more-than-once-in-sitemesh -->
    <decorator name="main" page="layout-bootstrap.dec">
        <pattern>/*</pattern>
    </decorator>
</decorators>

